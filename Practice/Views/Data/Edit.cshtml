
<h2>Edit</h2>

<form method="post">

    <input type="hidden" name="id" value="@Model.FormID" />
    <input type="text" name="Name" value="@Model.Name" class="form-control" placeholder="Name" />
    <input type="text" name="Number" value="@Model.Number" class="form-control" placeholder="Number" />
    <input type="hidden" name="Formname" value="Edited" />
    <input type="submit" class="btn btn-success" />

</form>